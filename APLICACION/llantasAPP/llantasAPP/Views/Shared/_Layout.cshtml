<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Interfas llantas Milenio">
    <meta name="author" content="Eduar Anaya">
    <link rel="shortcut icon" href="~/Content/medios/tire.ico" />
    <title>@ViewBag.title</title>
    <link href="https://fonts.googleapis.com/css?family=IBM+Plex+Sans" rel="stylesheet">
    <link href="~/CSS/css_Prove/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Scripts/js_Prove/jqueryUI/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/CSS/css_Prove/datatables/datatables.min.css" rel="stylesheet" />
    <link href="~/CSS/css_Edits/css_Gral.css?v1.6" rel="stylesheet" />
    <link href="~/CSS/css_Edits/css_Llantas_Edit.css?v1.4" rel="stylesheet" />
    <link href="~/CSS/css_Edits/css_Inventario.css?v1.1" rel="stylesheet" />
</head>
<body>
    <!-- Archivos Scripts -->
    <script src="~/Scripts/js_Prove/jQuery/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/js_Prove/jQuery/jquery.validate.min.js"></script>
    <script src="~/Scripts/js_Prove/bootstrap/bootstrap.min.js"></script>
    <script src="~/Scripts/js_Prove/jqueryUI/jquery-ui.min.js"></script>
    <script src="~/Scripts/js_Prove/modernizr-2.5.3.js"></script>
    <script src="~/Scripts/js_Prove/datatables/datatables.min.js"></script>
    <script src="~/Scripts/js_Prove/formatCurrency/jquery.formatCurrency-1.4.0.js"></script>
    <script src="~/Scripts/js_Prove/formatCurrency/jquery.formatCurrency.es-CO.js"></script>
    <script src="~/Scripts/js_Prove/plugTimer.js"></script>
    <script src="~/Scripts/js_Prove/tactil.js"></script>
    <!-- Navegacion -->
    <header>
        <nav id="barraMenu" class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
            <div class="container">
                <div class="row" style="width: 100%;">
                    <div class="col">
                        <a class="navbar-brand" href="@Url.Action("Index", "Home")">
                            <img src="~/Content/medios/Logo_Head.png" style="width: 6rem" alt="Inicio" /></a>
                    </div>
                    <div class="col">
                        <strong class="titulo">Llantas Operativo
                        </strong>
                    </div>
                </div>
            </div>
        </nav>
    </header>
    <section id="RenderBody">
        <div class="container Contapp">
            @RenderBody()
        </div>
    </section>
    <!-- Footer -->
    <footer class="bg-dark">
        <div class="container">
            <p class="m-0 text-center texFooter">
                Llantas Operativo Versión 3.0 @DateTime.Now.Year<br />
                &copy <a href="http://www.transer.com.co/" target="_blank" style="color: black;">Transportes y Servicios Transer S.A.</a>
            </p>
        </div>
        <!-- /.container -->
    </footer>

    <!-- Archivos Scripts -->
    <script src="~/Scripts/js_Edit/js_Edit_Llantas/js_SearchsLlantas.js?v3.0"></script>
    <script src="~/Scripts/js_Edit/js_Edit_Llantas/js_llantas_Edit.js?v3.1"></script>
    <script src="~/Scripts/js_Edit/js_timer.Js"></script>
</body>
</html>
